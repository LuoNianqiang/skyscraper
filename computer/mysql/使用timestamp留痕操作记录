CREATE TABLE b1 (  id INT (5) UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY,  NAME VARCHAR (10),  created_timestamp TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,  updated_timestamp TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP) ;
SELECT * FROM b1;
DROP TABLE b1;
INSERT INTO b1(NAME) VALUES('a');
UPDATE b1 SET NAME = 'name' WHERE id = 1;
INSERT INTO b1(NAME1) VALUES('b');
INSERT INTO b1(NAME) VALUES('c');
UPDATE b1 SET NAME = 'd' WHERE id = 3;
INSERT INTO b1(NAME, created_timestamp, updated_timestamp) VALUES ('test', SYSDATE(), SYSDATE());
UPDATE b1 SET NAME = 'ttt', created_timestamp= SYSDATE(), updated_timestamp= CURRENT_TIMESTAMP WHERE id =2;
